// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model information {
  id       Int    @id @default(autoincrement())
  services String
  budget   Int
  pages    Int
  message  String
  email    String
  createdAt DateTime @default(now())
}
